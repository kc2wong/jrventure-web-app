  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:full": "rimraf ./node_modules src/__generated__ dist",
    "openapi:generate-client": "openapi --input ./resources/linkedup-web-api/web-api-spec.yaml --output src/__generated__/linkedup-web-api-client --client axios --name WebApiClient",
    "start": "REACT_APP_NAME=$npm_package_name REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "NODE_ENV=production REACT_APP_NAME=$npm_package_name REACT_APP_VERSION=$npm_package_version node esbuild.config.mjs",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "deploy:ghPages": "gh-pages -d dist"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@testing-library/dom": "^10.4.0",
    "@types/fs-extra": "^11.0.4",
    "@types/luxon": "^3.6.2",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitest/coverage-v8": "^3.1.1",
    "esbuild": "^0.25.3",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-tsc": "^0.5.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-check-file": "^3.2.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "fs-extra": "^11.3.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "openapi-typescript-codegen": "^0.29.0",
    "typescript-eslint": "^8.30.1",
    "vitest": "^3.1.1"
  }
}
